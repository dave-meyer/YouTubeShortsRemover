{

  "description": "Removes YouTube Shorts section from YouTube homepage",
  "manifest_version": 2,
  "name": "YouTube Shorts Remover",
  "version": "0.1",
  "homepage_url": "https://github.com/dave-meyer",
  "icons": {
    "48": "icons/border-48.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "js": ["shorts-remover.js"]
    }
  ]

}
